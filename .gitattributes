print("Enter the Numbers")
a = int(input("Enter first no."))
b = int(input("Enter sec no."))
c = int(input("Enter third no."))

if a>b and a>c:
    print(a,"is greater")
else:
    if b>a and b>c:
        print(b," is greater")
    else:
        if c>a and c>b:
            print(c, "is greater")
        else:
            print("two or more numbers are equals")
        

